<h1><%= title %></h1>

<% if (messages) { %>
  <div class="notice">
    <%- messages() %>
  </div>
<% } %>


<form action="https://cse340neww.onrender.com/account/login" method="post" class="form">
  <label for="account_email">Email</label>
  <input type="email" name="account_email" id="account_email" required>

  <label for="account_password">Password</label>
  <input type="password" name="account_password" id="account_password" required>

  <button type="submit">Login</button>
</form>

<p>
  Don't have an account?  
  <a href="/account/register">Register Here</a>
</p>
